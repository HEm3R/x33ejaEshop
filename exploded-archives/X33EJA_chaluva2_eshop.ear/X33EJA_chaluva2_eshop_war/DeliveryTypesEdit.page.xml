<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/DeliveryTypesList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{deliveryTypesHome.wire}"/>

   <param name="deliveryTypesFrom"/>
   <param name="deliveryTypesId" value="#{deliveryTypesHome.deliveryTypesId}"/>


   <navigation from-action="#{deliveryTypesHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DeliveryTypes.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{deliveryTypesHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DeliveryTypes.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{deliveryTypesHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DeliveryTypesList.xhtml"/>
      </rule>
   </navigation>

</page>
